@model IEnumerable<Formservicecrud.Models.Dropdown>

@{
    ViewBag.Title = "SelectCategory";
}

<h2>SelectCategory</h2>

<p>
    @Html.ActionLink("Create New", "Insertcategory")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dropdown)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Image)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
        <td>
            @Html.DisplayFor(Modelitem => item.id)
        </td>
        <td>
            @Html.DisplayFor(Modelitem => item.dropdown)
        </td>
        <td>
            @Html.DisplayFor(Modelitem => item.Gender)
        </td>
        <td>
            @Html.DisplayFor(Modelitem => item.Date)
        </td>
        <td>
            <img src="@item.Image" width="100" height="100" />
        </td>
        <td>
            @Html.ActionLink("Edit", "Updatecategory", new { id = item.id }) |
      
            @Html.ActionLink("Delete", "Deletedata", new { id = item.id })
        </td>
</tr>
}

</table>